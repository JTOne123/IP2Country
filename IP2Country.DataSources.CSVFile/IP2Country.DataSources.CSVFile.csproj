<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net451;net452;net46;net461;net462;net47;net471;netstandard2.0;netcoreapp2.0</TargetFrameworks>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>IP-2-Country.DataSources.CSVFile</PackageId>
    <Product>IP-2-Country.DataSources.CSVFile</Product>
    <Copyright>(C) 2018 Devcorner.nl</Copyright>
    <Authors>RobIII</Authors>
    <Company>Devcorner.nl</Company>
    <PackageLicenseUrl>https://github.com/RobThree/IP2Country/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://raw.githubusercontent.com/RobThree/IP2Country/master/icons/csvdatasource.png</PackageProjectUrl>
    <PackageTags>IP Geolocation Country IP2Country IpToCountry</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <Description>Provides base classes for handling CSV files (plain, gzipped or zipped) for IP2Country</Description>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.Compression" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\IP2Country\IP2Country.csproj" />
  </ItemGroup>

</Project>
